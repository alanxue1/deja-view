# Copy this file to `.env.local` and fill in real values.
#
#   cp env.example .env.local
#
# Then restart dev server:
#
#   npm run dev
#

# Google Programmable Search Engine (Custom Search JSON API)
GOOGLE_CSE_API_KEY=
GOOGLE_CSE_ID=

# Google Gemini API (for AI-powered item placement)
GOOGLE_GEMINI_API_KEY=

# Clerk
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Mongo (optional / future)
MONGODB_PROD_URI=
MONGODB_DEV_URI=

# Node environment (optional; Next sets this automatically in dev)
NODE_ENV=development

REPLICATE_API_TOKEN=
